{
  "name": "@revisium/admin",
  "version": "2.2.0-alpha.1",
  "description": "Revisium is a tool (UI/API) inspired by JSON (JSON Schema) and Git, designed to provide a flexible and low-level headless CMS solution.",
  "private": false,
  "homepage": "https://revisium.io",
  "license": "Apache-2.0",
  "author": "Anton Kashirov",
  "repository": {
    "type": "git",
    "url": "https://github.com/revisium/revisium-admin.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "dev": "vite",
    "build": "tsc && vite build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "ts:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --detectOpenHandles --ci",
    "test:e2e": "playwright test --config=e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config=e2e/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report",
    "preview": "vite preview",
    "prettier": "prettier . --write",
    "gql:codegen": "graphql-codegen && npm run prettier",
    "gql:codegen:download": "graphql-codegen --download",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@apollo/sandbox": "2.7.3",
    "@atlaskit/drag-and-drop": "0.15.1",
    "@casl/ability": "6.7.3",
    "@chakra-ui/react": "3.30.0",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/lang-markdown": "6.3.2",
    "@emotion/react": "11.14.0",
    "@uiw/codemirror-theme-github": "4.25.4",
    "@uiw/react-codemirror": "4.23.12",
    "@xyflow/react": "12.10.0",
    "@zag-js/menu": "1.31.1",
    "ajv": "8.16.0",
    "date-fns": "3.3.1",
    "diff": "8.0.2",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "jwt-decode": "4.0.0",
    "mobx": "6.12.3",
    "mobx-react-lite": "4.0.7",
    "mobx-utils": "6.0.8",
    "nanoid": "3.3.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "react-router-dom": "6.30.3",
    "react-top-loading-bar": "2.3.1",
    "react-use": "17.5.0",
    "react-virtuoso": "4.12.8",
    "vite": "7.2.2",
    "vite-plugin-checker": "0.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-graphql-request": "6.2.0",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@playwright/test": "1.57.0",
    "@types/diff": "7.0.2",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "@vitejs/plugin-react-swc": "4.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-sonarjs": "3.0.5",
    "jest": "29.7.0",
    "patch-package": "8.0.1",
    "prettier": "3.2.5",
    "sass": "1.71.1",
    "ts-jest": "29.1.5",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2",
    "vite-tsconfig-paths": "5.1.4"
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 0.3%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "^.+\\.(t|j)sx$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "^src/shared/lib/DIContainer\\.ts$": "<rootDir>/__mocks__/DIContainerMock.ts",
      "^src/shared/lib$": "<rootDir>/__mocks__/sharedLibMock.ts",
      "^src/shared/model$": "<rootDir>/__mocks__/sharedModelMock.ts",
      "^src/shared/model/EnvironmentService\\.ts$": "<rootDir>/__mocks__/EnvironmentServiceMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  }
}
