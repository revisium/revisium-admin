{
  "name": "@revisium/admin",
  "version": "0.11.0-alpha.3",
  "description": "Revisium is a tool (UI/API) inspired by JSON (JSON Schema) and Git, designed to provide a flexible and low-level headless CMS solution.",
  "private": false,
  "homepage": "https://revisium.io",
  "license": "Apache-2.0",
  "author": "Anton Kashirov",
  "repository": {
    "type": "git",
    "url": "https://github.com/revisium/revisium-admin.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "ts:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --detectOpenHandles --ci",
    "preview": "vite preview",
    "prettier": "prettier . --write",
    "gql:codegen": "graphql-codegen && npm run prettier",
    "gql:codegen:download": "graphql-codegen --download",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@apollo/client": "3.10.4",
    "@atlaskit/drag-and-drop": "0.15.1",
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "ajv": "8.16.0",
    "date-fns": "3.3.1",
    "framer-motion": "10.16.4",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "jwt-decode": "4.0.0",
    "mobx": "6.12.3",
    "mobx-react-lite": "4.0.7",
    "mobx-state-tree": "5.4.2",
    "mobx-utils": "6.0.8",
    "nanoid": "3.3.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "react-router-dom": "6.23.1",
    "react-top-loading-bar": "2.3.1",
    "react-use": "17.5.0",
    "react-virtuoso": "4.7.10",
    "vite": "6.3.2",
    "vite-plugin-checker": "0.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-graphql-request": "6.2.0",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-codegen/typescript-react-apollo": "4.3.0",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "@vitejs/plugin-react-swc": "3.9.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "reactotron-mst": "3.1.9",
    "reactotron-react-js": "3.3.14",
    "sass": "1.71.1",
    "ts-jest": "29.1.5",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2",
    "vite-tsconfig-paths": "5.1.4"
  },
  "engines": {
    "node": "20.11.1"
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 0.3%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "^.+\\.(t|j)sx$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  }
}
